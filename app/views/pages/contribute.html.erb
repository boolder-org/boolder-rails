<% set_meta_tags title: t("views.contribute.metas.title") %>
<% set_meta_tags description: t("views.contribute.metas.description") %>

<div class="max-w-3xl mx-auto sm:px-6 lg:px-8 pb-8">
  <div class="my-4 px-4 sm:px-0">
    
    <h1 class="mb-4 py-4 text-4xl font-bold text-gray-900 inline-flex items-center space-x-4">
      <span><%= t("views.contribute.title") %></span>
      <span class="inline-flex items-base rounded-md bg-emerald-50 px-2 py-1 text-xs font-medium text-emerald-700 ring-1 ring-inset ring-emerald-600/20">BETA</span>
    </h1>


<div class="divide-y divide-gray-200 overflow-hidden rounded-lg bg-gray-200 shadow sm:grid sm:grid-cols-2 sm:gap-px sm:divide-y-0">
  <div class="rounded-tl-lg rounded-tr-lg sm:rounded-tr-none group relative bg-white p-6 focus-within:ring-2 focus-within:ring-inset focus-within:ring-emerald-500">
    <div>
      <span class="inline-flex rounded-lg p-3 bg-emerald-50 text-emerald-700 ring-4 ring-white">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
